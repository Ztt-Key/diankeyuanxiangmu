model ThreeTicketExample {
  ID            BigInt    @id @default(autoincrement()) @map("id")
  CreatedAt     DateTime? @map("created_at") @db.Timestamptz(6)
  UpdatedAt     DateTime? @updatedAt @map("updated_at") @db.Timestamptz(6)
  DeletedAt     DateTime? @map("deleted_at") @db.Timestamptz(6)
  value         Json?
  log           Json?
  templeId      String?   @map("temple_id")
  applicantId   BigInt?   @map("applicant_id")
  applicantName String?   @map("applicant_name")

  @@index([DeletedAt], map: "idx_three_ticket_examples_deleted_at")
  @@map("three_ticket_examples")
}

